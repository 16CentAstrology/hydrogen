{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      // This config allows VSCode users to use the built-in debugger to launch the `yarn dev-server` script
      "type": "node",
      "name": "Debug dev-server",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["dev-server"],
      // Causes the debug script to not execute any code upon launching - which allows you to debug code that executes on program start. If you find that annoying or not useful, you can temporarily remove/comment this line out
      "stopOnEntry": true,
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**",
        // In general it's nice to skip react-dom code as you step over, but comment this line out if you need to dig into react-dom itself and see what's happening
        "${workspaceFolder}/node_modules/react-dom/**"
      ]
    }
  ]
}
